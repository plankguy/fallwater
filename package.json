{
  "name": "fallwater",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "highlight.js": "~9.12.0",
    "js-cookie": "~2.1.4",
    "node-sass-chokidar": "0.0.3",
    "node-sass-json-importer": "^3.1.5",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.1",
    "prismic-javascript": "^1.4.0",
    "prismic-reactjs": "^0.2.0",
    "prismic-toolbar": "0.0.2",
    "prismic.io": "^3.5.7",
    "prop-types": "^15.6.0",
    "qs": "^6.4.0",
    "react": "~16.0.0",
    "react-dom": "~16.0.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "~4.2.2",
    "react-transition-group": "^2.2.1",
    "styled-components": "^3.2.3",
    "styled-transition-group": "^1.0.0",
    "whatwg-fetch": "~2.0.3"
  },
  "devDependencies": {
    "react-scripts": "~1.0.14"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --importer ./node_modules/node-sass-json-importer/dist/node-sass-json-importer.js --include-path ./src --include-path ./node_modules src/ --output src/",
    "watch-css": "npm run build-css && node-sass-chokidar --importer ./node_modules/node-sass-json-importer/dist/node-sass-json-importer.js --include-path ./src --include-path ./node_modules src/ --output src/ --watch ./src --recursive",
    "watch-js": "react-scripts start",
    "start": "npm-run-all -p watch-css watch-js",
    "build": "npm run build-css && react-scripts build && echo '/*    /index.html   200' > build/_redirects",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "engines": {
    "node": ">=4.8.1",
    "npm": ">=2.15.11"
  }
}
