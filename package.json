{
  "name": "fallwater",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@researchgate/react-intersection-observer": "^0.6.0",
    "highlight.js": "~9.12.0",
    "hoek": "^5.0.3",
    "intersection-observer": "^0.5.0",
    "js-cookie": "~2.1.4",
    "node-sass": "^4.8.3",
    "node-sass-chokidar": "^1.2.2",
    "node-sass-js-importer": "^3.0.2",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.1",
    "popmotion-pose": "^1.3.0",
    "prismic-javascript": "^1.4.0",
    "prismic-reactjs": "^0.2.0",
    "prismic-toolbar": "0.0.2",
    "prismic.io": "^3.5.7",
    "prop-types": "^15.6.0",
    "qs": "^6.4.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-helmet": "^5.2.0",
    "react-lazy-load": "^3.0.13",
    "react-lazyload": "^2.3.0",
    "react-pose": "^1.3.0",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.2.1",
    "styled-components": "^3.2.3",
    "styled-transition-group": "^1.0.0",
    "whatwg-fetch": "~2.0.3"
  },
  "devDependencies": {
    "react-scripts": "~1.0.14"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --importer ./node_modules/node-sass-js-importer/dist/node-sass-js-importer.js --include-path ./src --include-path ./node_modules src/ --output src/ --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar --importer ./node_modules/node-sass-js-importer/dist/node-sass-js-importer.js --include-path ./src --include-path ./node_modules src/ --output src/ --watch ./src --recursive",
    "watch-js": "react-scripts start",
    "start": "npm-run-all -p watch-css watch-js",
    "build": "npm run build-css && react-scripts build && echo '/*    /index.html   200' > build/_redirects",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "engines": {
    "node": ">=4.8.1",
    "npm": ">=2.15.11"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost:3000",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^components[/](.+)": "<rootDir>/src/components/$1",
      "^utils[/](.+)": "<rootDir>/src/utils/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
